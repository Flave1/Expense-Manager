                        <div class="col-sm-6 card-block-big card-block-sm br">
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    <h5>
                                        @Model.AppStageStat.BiodataCount
                                    </h5>
                                    <span>Bio-Data</span>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 card-block-big card-block-sm">
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    <h5>
                                        @Model.AppStageStat.PDetailCount
                                    </h5>
                                    <span>Personal Details</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="row-table">
                        <div class="col-sm-6 card-block-big card-block-sm br">
